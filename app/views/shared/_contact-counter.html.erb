<section id="counter" class="wrapper style1 align-center counter-contact">
  <div id="counter-section" class="inner white-text">
    <!-- <div class=""> -->
      <div class="counter-container">
        <div class="mb30 wow fadeInUp display-flex" data-wow-delay="0.2s">
            <h1 class="green-text"><span id="trainer-counter" class="counter">0</span></h1>
            <h3 class="font600  white-text"><%= t("counter.missions") %></h3>
        </div>

        <div class="mb30 wow fadeInUp display-flex" data-wow-delay="0.1s">
            <h1 class="green-text"><span id="training-counter" class="counter">0</span></h1>
            <h3 class="font600  white-text"><%= t("counter.coaches") %></h3>
        </div>

        <div class="mb30 wow fadeInUp display-flex" data-wow-delay="0.3s">
            <h1 class="green-text"><span id="client-counter" class="counter">0</span></h1>
            <h3 class="font600  white-text"><%= t("counter.participants") %></h3>
        </div>

        <div class="mb30 wow fadeInUp display-flex" data-wow-delay="0.3s">
            <h1 class="green-text"><span id="trainee-counter" class="counter">0</span></h1>
            <h3 class="font600  white-text"><%= t("counter.mission_participants") %></h3>
        </div>
      </div>
    <!-- </div> -->
  </div>
  <!-- Form -->
  <section id="contact" class="wrapper style1 align-left contact-in-counter">
    <div class="inner">
      <header>
        <h2 class="align-center">Contact us</h2>
      </header>
      <div class="content">
        <%= render 'shared/contact-form' %>
      </div>
    </div>
  </section>
</section>
